<script type="text/javascript">
    $(document).ready(function () {
        $(".blogEntry-16035-error").hide();

        $(".attach-blogEntry-16035-to-contest-link").click(function () {
            Codeforces.facebox(".attach-blogEntry-16035-to-contest-form-holder", "http://st.codeforces.com");

            $("#facebox .attach-blogEntry-16035-to-contest-form").submit(function () {
                if ($(this).find(":disabled").length === 0) {
                    $("#facebox .blogEntry-16035-error").hide();
                    $("#facebox #blogEntry-16035-resource-submitting-indicator").show();
                    $("#facebox #blogEntry-16035-resource-submit").attr("disabled", "disabled");

                    //noinspection JSUnusedGlobalSymbols
                    $.post("/data/blogAndContest?action=attachBlogToContest", {
                        blogId:"686", contestId:$("#facebox #blogEntry-16035-contestId").val(),
                        blogEntryId:"16035",
                        resourceType:$("#facebox #blogEntry-16035-resourceType").val()
                    }, function (json) {
                        var validated = true;

                        for (var key in json) {
                            if (json.hasOwnProperty(key) && key.indexOf("error__") === 0) {
                                validated = false;
                                $("#facebox .blogEntry-16035-" + key.substring("error__".length) + "-error").show();
                                $("#facebox .for__blogEntry-16035-" + key.substring("error__".length)).text(json[key]);
                            }
                        }

                        $("#facebox #blogEntry-16035-resource-submitting-indicator").hide();
                        $("#facebox #blogEntry-16035-resource-submit").removeAttr("disabled");

                        if (validated) {
                            $.facebox.close();
                            if (json["success"] === "true") {
                                Codeforces.showMessage("Blog has been successfully attached to selected contest");
                            } else {
                                Codeforces.showMessage("There is unexpected error while attaching blog to the contest");
                            }
                        }
                    }, "json");
                }

                return false;
            });

            var contestSelect = $("#blogEntry-16035-contestId");
            var popupContestSelect = $("#facebox #blogEntry-16035-contestId");

            if (popupContestSelect.is(":disabled")) {
                $.post("/data/contests?action=getContestListWithEditableResources", function (data) {
                    if (data["result"] === "success") {
